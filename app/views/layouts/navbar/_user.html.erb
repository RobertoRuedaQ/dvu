<% if current_user %>
  <li>
     <%= link_to "Hola #{current_user.first_name}!", edit_user_registration_path(current_user) %>
  </li>
  <li>
    <%= link_to "Cerrar Sesión", destroy_user_session_path, class: "btn" %>
  </li>
  <%else %>
  <li> 
    <%= link_to "Iniciar Sesión", new_user_session_path, class: "btn" %>
  </li>
  <li>
    <%= link_to "Registrarse", new_user_registration_path, class:"btn" %>
  </li>
<%end %>